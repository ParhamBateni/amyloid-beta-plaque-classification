{
    "data": {
        "data_folder": "data",
        "data_table_file_name": "data_table_sampled_10k.csv",
        "labeled_data_folder": "labeled_images",
        "unlabeled_data_folder": "sampled_unlabeled_images",
        "runs_folder": "runs",
        "downscaled_image_size": [224, 224],
        "downscaling_method": "bilinear",
        "unlabeled_sample_size": 2000,
        "labeled_sample_size": 1e9,
        "preload_data": true,
        "apply_transforms_on_the_fly": false,
        "normalize_data": true,
        "normalize_mean": [0.485, 0.456, 0.406],
        "normalize_std": [0.229, 0.224, 0.225],
        "use_extra_features": true,
        "extra_feature_dim": 2,
        "number_of_augmentations": 3
    },
    "training": {
        "test_size": 0.2,
        "val_size": 0.1,
        "num_workers": 4,
        "pin_memory": true,
        "persistent_workers": true,

        "batch_size": 64,
        "num_epochs": 500,
        "optimizer": "adamw",
        "learning_rate": 1e-4,
        "weight_decay": 1e-5,
        "early_stop": 2,
        "early_stop_check_val_every_n_epoch": 5,
        "early_stop_monitor": "val_f1",
        "cv_folds": 5
    },
    "system": {
        "debug_mode": true,
        "log_mode": true,
        "random_seed": 44,
        "seed_everything": true
    },
    "cv_grid_search": {
    }
}
